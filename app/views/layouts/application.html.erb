<!DOCTYPE html>
<html>
  <head>
    <title>GaslightTwitter</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
  	<%= render partial: "layouts/navbar" %>
    <div class="container">
      <% flash.each do |key, value| %>
        <div class="alert">
          <%= value %>
        </div>
      <% end %>
      <%= yield %>
    </div>
  </body>
</html>
